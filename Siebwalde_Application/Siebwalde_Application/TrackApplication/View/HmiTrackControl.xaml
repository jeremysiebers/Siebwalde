<UserControl x:Class="Siebwalde_Application.TrackApplication.View.HmiTrackControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:uc="clr-namespace:Siebwalde_Application.TrackApplication.View"
             mc:Ignorable="d" 
             d:DesignHeight="542" d:DesignWidth="1081">
    <Grid>
        <DataGrid x:Name="ViewTrackamplifiers" HorizontalAlignment="Left" Height="480" Margin="28,22,0,0" VerticalAlignment="Top" Width="988"
                  ItemsSource="{Binding trackAmplifiers}" AutoGenerateColumns="False">
            <DataGrid.Columns>
                <DataGridTextColumn Header="Slave No" Binding="{Binding SlaveNumber}"/>
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</UserControl>
